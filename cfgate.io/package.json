{
  "name": "cfgate-infra",
  "version": "0.1.1",
  "description": "https://cfgate.io",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy:cf": "wrangler deploy",
    "format": "prettier --write .",
    "lint": "eslint --fix",
    "types": "wrangler types src/worker-configuration.d.ts",
    "typecheck": "pnpm types && tsc --noEmit",
