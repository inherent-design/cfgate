    "test:watch": "vitest",
    "tail": "wrangler tail",
    "tail:prod": "wrangler tail --env production",
    "prepare": "pnpm types "
  },
  "author": "inherent.design",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.28.2",
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.12.9",
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.2.0",
    "@vitest/runner": "^3.2.4",
    "@vitest/snapshot": "^3.2.4",
    "eslint": "^9.39.2",
    "eslint-plugin-cflint": "^1.0.0",
    "globals": "^17.3.0",
    "jiti": "^2.6.1",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.63.0"
  },
  "dependencies": {
    "hono": "^4.11.7"
  }
